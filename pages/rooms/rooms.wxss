/* pages/rooms/rooms.wxss *//* pages/charts/charts.wxss */
.container {
  background-image: url('https://s1.imagehub.cc/images/2023/09/18/eda6f94f7e355a790cf2600d52a1991.png');
  background-size: auto 100%;
  background-repeat: no-repeat;
  background-position: center;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.input-box {
  width: 20%;
  text-align: center;
  color: #999; /* 设置字体颜色为浅色 */
  background-color: #f5f5f5; /* 设置背景色 */
  position:absolute;
  top: 50%;
}
.my_view {
  width: 100%;
  position: absolute;
  top: 40%;
  text-align: center;
  justify-content: center;
}
.artfont {
  font-family:Arial, Helvetica, sans-serif;
  font-size: 40px;
  color:red;
  -webkit-text-stroke-width: 2px;
  -webkit-text-stroke-color: lightgreen;
}
.button {
  background-color:rgba(128, 128, 128, 0.6);
  border-radius: 10%;
  color: turquoise;
  border: 1px solid transparent;
  padding: 10px; 
  position: absolute;
  top: 55%;
}
.golden {
  background-color:gold;
  border-radius: 10%;
  color: turquoise;
  border: 1px solid transparent;
  padding: 10px; 
  position: absolute;
  top: 55%;
}